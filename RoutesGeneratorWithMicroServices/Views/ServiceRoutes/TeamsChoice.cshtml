@model RoutesGeneratorWithMicroServices.Models.Team

@{
	ViewData["Title"] = "TeamsChoice";
}

<h1>TeamsChoice</h1>

<h4>Teams</h4>
<hr />
<table class="table">
	<tbody>
		@foreach (var team in ViewBag.Teams)
		{
			<tr>
				<td>
					<input type="checkbox" id="Team" name="Tame" value=@team.Name>
					<label for="@team.Name"> @team.Name</label>
					<br />
				</td>
			</tr>
		}
	</tbody>
</table>
